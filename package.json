    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.4.2",
    "ts-morph": "^24.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "@credal/sdk": "^0.0.21",
